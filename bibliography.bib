title = {{Material Property Changes in Custom-Designed Digital Composite Structures Due to Voxel Size}},
year = {2018}
}
@article{Kaweesa2018a,
abstract = {The capability of Additive Manufacturing (AM) to manufacture multi-materials allows the fabrication of complex and multifunctional objects with heterogeneous material compositions and varying mechanical properties. The material jetting AM process specifically has the capability to manufacture multi-material structures with both rigid and flexible material properties. Existing research has investigated the fatigue properties of 3D printed multi-material specimens and shows that there is a weakness at multi-material interfaces. This paper seeks to, instead, investigate the effects of gradual material transitions on the fatigue life of 3D printed multi-material specimens. In order to examine the fatigue life at the multi-material interface, stepwise gradients are compared against continuous gradients created through voxel-based design. Results demonstrate the effects of different material gradient patterns and different material transition lengths on the fatigue life of multi-material specimens. In addition, the behavior of individual material composites is studied to confirm how gradient designs based on different material compositions affect their material properties.},
author = {Kaweesa, Dorcas V. and Meisel, Nicholas A.},
doi = {10.1016/j.addma.2018.03.011},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Kaweesa, Meisel - 2018 - Quantifying fatigue property changes in material jetted parts due to functionally graded material interface des.pdf:pdf},
issn = {22148604},
journal = {Additive Manufacturing},
keywords = {Fatigue life,Functionally graded materials,Material jetting,PolyJet process,Voxel design},
number = {March},
pages = {141--149},
publisher = {Elsevier},
title = {{Quantifying fatigue property changes in material jetted parts due to functionally graded material interface design}},
url = {https://doi.org/10.1016/j.addma.2018.03.011},
volume = {21},
