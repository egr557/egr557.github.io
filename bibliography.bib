isbn = {0277786X (ISSN)},
issn = {0277786X},
journal = {Defense and Security},
keywords = {bioinspired design,climbing robots,gecko adhesion,power autonomous locomotion,scansorial agility},
pages = {291--302},
title = {{Robotics in Scansorial Environments}},
url = {http://proceedings.spiedigitallibrary.org/proceeding.aspx?articleid=864272},
volume = {5804},
year = {2005}
}
