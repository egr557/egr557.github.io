keywords = {Animals,Biomechanics,Cockroaches,Cockroaches: physiology,Locomotion,Locomotion: physiology,Time Factors},
month = {aug},
number = {Pt 15},
pages = {2990--3000},
pmid = {16857883},
title = {{Dynamics of rapid vertical climbing in cockroaches reveals a template.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16857883},
volume = {209},
