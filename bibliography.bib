@article{Yoshimoto2003,
abstract = {To obtain a good approximation for data fitting with a spline, frequently we have to deal with knots as variables. The problem to be solved then becomes a continuous nonlinear and multivariate optimization problem with many local optima. Therefore, it is difficult to obtain the global optimum. In this paper, we propose a method for solving this problem by using a real-coded genetic algorithm. Our method can treat not only data with a smooth underlying function, but also data with an underlying function having discontinuous points and/or cusps. We search for the best model among candidate models by using the Bayes Information Criterion (BIC). With this, we can appropriately determine the number and locations of knots automatically and simultaneously. Five examples of data fitting are given to show the performance of our method. ?? 2003 Elsevier Science Ltd. All rights reserved.},
author = {Yoshimoto, Fujiichi and Harada, Toshinobu and Yoshimoto, Yoshihide},
doi = {10.1016/S0010-4485(03)00006-X},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Yoshimoto, Harada, Yoshimoto - 2003 - Data fitting with a spline using a real-coded genetic algorithm.pdf:pdf},
issn = {00104485},
journal = {CAD Computer Aided Design},
keywords = {Genetic algorithm,Geometric modeling,Knot placement,Spline},
number = {8 SPEC.},
pages = {751--760},
title = {{Data fitting with a spline using a real-coded genetic algorithm}},
volume = {35},
year = {2003}
}
