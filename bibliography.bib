journal = {Sustainability (Switzerland)},
keywords = {Participatory modeling,Sustainability education,Sustainable futures,Transformational sustainability science,Visioning,Visions},
number = {7},
pages = {4452--4469},
title = {{Studying, teaching and applying sustainability visions using systems modeling}},
volume = {6},
year = {2014}
}
