month = {jun},
number = {Pt 11},
pages = {1459--1463},
pmid = {10229692},
title = {{Branching out in locomotion: the mechanics of perch use in birds and primates.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10229692},
volume = {202},
year = {1999}
}
