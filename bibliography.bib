@article{Boblan2004,
abstract = {Humanoid robots are fascinating from two points of view, firstly their construction and secondly because they lend life to inanimate objects. The combination of biology and robots leads to smoother and compliant movement which is more pleasant for us as people. Biologically inspired robots embody non-rigid movement which are made possible by special joints or actuators which give way and can both actively and passively adapt stiffness in different situations. The following paper deals with the construction of a compliant embodiment of a humanoid robot arm, including a five-finger hand with artificial fluidic muscles. The biologically inspired decentralized control architecture allows small units to be responsible for each main movement task. The first section gives a short introduction as to how bionics engineers think and tries to motivate us to build compliant machines. The second section looks at mechanical aspects, limitations and constraints and furthermore describes a human-like robot arm and hand. Section 3 presents the fluidic muscle actuator of the company FESTOThis project was supported by Dr. Werner Fischer and Univ. Prof. Axel Thallemer, FESTO AG & Co. KG.. The fourth section describes the decentralized control architecture and the electronic components. The last section concludes the paper while looking at further prospects.},
author = {Boblan, Ivo and Bannasch, Rudolf and Schwenk, Hartmut and Prietzel, Frank and Miertsch, Lars and Schulz, Andreas},
doi = {10.1007/978-3-540-27833-7_12},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Boblan et al. - 2004 - A human-like robot hand and arm with fluidic muscles Biologically inspired construction and functionality.pdf:pdf},
journal = {Embodied Artificial Intelligence},
pages = {629--629},
publisher = {Springer},
title = {{A human-like robot hand and arm with fluidic muscles: Biologically inspired construction and functionality}},
url = {http://www.springerlink.com/content/xeet7n49v4vmxgkg http://www.springerlink.com/index/xeet7n49v4vmxgkg.pdf},
year = {2004}
}
@article{Scansorial1959,
abstract = {"The scansorial foot of the woodpeckers is not a zygodactyl foot,$\$nas commonly believed, but a quite different structure - the ectropodactyl$\$nfoot. With the exception of the most generalized members of the$\$nPicinae, which retain the ancestral zygodactyl foot as the climbing$\$nfoot, the toes of a climbing woodpecker are arranged as follows:$\$ntoes two and three point forward, the fourth toe is thrust out to$\$nthe lateral side at right angles to the fore toes, and the hallux$\$nusually lies beneath the distal end of the tarsometatarsus in a$\$ncramped position and is functionless. In the ivory-billed woodpecker,$\$nthe hallux is long and functional and is directed laterally next$\$nto the fourth toe. The fourth toe is either directed forward or$\$nthrust out to the side. In all the climbing woodpeckers, the fore$\$ntoes, together with the stiffened tail feathers which are propped$\$nagainst the tree trunk, serve to support the bird against the downward$\$nand inward component of gravity. The laterally directed fourth toes,$\$nand to a slight extent the fore toes, prevent the bird from being$\$npulled away from the trunk by the outward component of gravity.$\$nAny toe pointing down the tree trunk would be functionless. Lastly,$\$nthe evolution of the ectropodactyl foot from the zygodactyl foot$\$nis outlined. 2. The evolution of the perching- and climbing-foot$\$ntypes in birds is described. It is shown that the main requirement$\$nof a perching foot is a set of strong opposable toes. The anisodactyl,$\$nsyndactyl, heterodactyl, and zygodactyl arrangements of the toes$\$nfill the requirements of a perching foot. On the other hand, the$\$ntoes of a climbing foot must be arranged to oppose the pull of gravity$\$nand should bear strongly curved claws. The anisodactyl, syndactyl,$\$npamprodactyl, and ectropodactyl foot types comply with the functional$\$ndemands of a climbing foot. A dendrogram showing the foot types$\$nand their evolution is given. It is shown that each arrangement$\$nof the toes evolved in response to a particular function (i.e.,$\$nanisodactyl foot evolved for perching), but once evolved it was$\$nalso suitable for other functions (i.e., running or climbing). 3.$\$nA brief discussion of the principle of multiple pathways of adaptation$\$nor evolution is presented. It is shown that there may be several$\$nmorphologically different answers to the same selection force and$\$nthat the morphological differences between these adaptive answers$\$nare not the result of differences in function but are a result of$\$nphylogenetically different starting points. Furthermore, it is shown$\$nthat one cannot conclude that different structures (i.e., arrangements$\$nof the toes) are non-adaptive just because their morphological differences$\$nare non-adaptive. Examples from the evolution of the perching- and$\$nclimbing-foot types were chosen to illustrate these conclusions"--P.$\$n42-43.},
author = {Bock, Walter Joseph and Miller, Waldron D.},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Bock, Miller - 1959 - The scansorial foot of the woodpeckers, with comments on the evolution of perching and climbing feet in birds.pdf:pdf},
issn = {0003-0082},
journal = {AmMusNov},
number = {1931},
pages = {--------},
title = {{The scansorial foot of the woodpeckers, with comments on the evolution of perching and climbing feet in birds}},
year = {1959}
}
@article{Bocskai2013,
abstract = {In the present paper, we discuss the numeric integration techniques of the 9-node quadrilateral isoparametric Lagrangian Mindlin shell element. We present the concepts of full, reduced and selective integration schemes from two fundamental standpoints. The first being the well known inherent property of the displacement formulation of linear finite elements, notably the stiffening of the element (also known as shear-locking) when full integration of the stiffness matrix is used, while the second is concerned with reduced integration, where compensating for the previous problem by not accounting fully for certain higher order terms of the integrand the element becomes softer. In this latter case certain spurious zero energy displacement modes (ZEM) are possible to arise leading to unrealistic results, therefore it has to be handled by adding restrictive terms to the stiffness matrix. We conclude, that in case of the element under scrutiny, by selective integration (reduction at the bending shear terms) one ZEM occurs, which propagates through the whole mesh, while the full integration stiffens the element only by less than 6%, therefore we recommend the use of the 3x3 full integration scheme.},
author = {Bocskai, Zolt{\'{a}}n and R{\'{o}}bert, Nagy},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Bocskai, R{\'{o}}bert - 2013 - Discussion of the numerical integration of the 9-node shell finite element.pdf:pdf},
journal = {Second Conference of Junior Researchers in Civil Engineering},
pages = {49--54},
title = {{Discussion of the numerical integration of the 9-node shell finite element}},
url = {https://www.me.bme.hu/doktisk/konf2013/%5Cnhttps://www.me.bme.hu/doktisk/konf2013/papers/049-054.pdf},
year = {2013}
}
@article{Bodnar2016,
abstract = {\nBackground\nMany students may not respond strongly to instruction that they do not perceive as engaging. One pedagogical approach to help engage students involves the use of games. Educational games can provide students with a motivating and stimulating environment while providing them with immediate feedback to promote learning.\n\nPurpose\nThis systematic review examines research focused on the implementation of games to teach undergraduate engineering students; it summarizes prevailing features and cites examples from a variety of engineering disciplines.\n\nScope/Method\nThe systematic review was conducted through a detailed search of Science Direct journals and the Scopus, Web of Science, Compendex/Inspec, and ERIC Education Research Abstract databases using terms pertinent to games in engineering education. A total of 191 papers was included after application of the inclusion/exclusion criteria. After screening those to determine if assessment of student learning outcomes was performed, 62 papers were found suitable for more detailed analysis.\n\nConclusions\nResearch on the implementation of games in undergraduate engineering classrooms has shown that, despite diverse forms of assessment applied, there is a general trend that both student learning and attitudes are improved by game-based activities. However, since only a relatively small subset of the literature demonstrates a systematic, validated approach in assessment, significant opportunities remain for future research.\n},
author = {Bodnar, Cheryl A. and Anastasio, Daniel and Enszer, Joshua A. and Burkey, Daniel D.},
doi = {10.1002/jee.20106},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Bodnar et al. - 2016 - Engineers at Play Games as Teaching Tools for Undergraduate Engineering Students.pdf:pdf},
isbn = {2168-9830},
issn = {10694730},
journal = {Journal of Engineering Education},
keywords = {Games,Gamification,Student experience,Systematic review,Undergraduate},
number = {1},
pages = {147--200},
title = {{Engineers at Play: Games as Teaching Tools for Undergraduate Engineering Students}},
volume = {105},
year = {2016}
}
