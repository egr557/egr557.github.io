isbn = {9781467380263},
issn = {10504729},
journal = {Proceedings - IEEE International Conference on Robotics and Automation},
pages = {4963--4968},
title = {{A composite soft bending actuation module with integrated curvature sensing}},
volume = {2016-June},
year = {2016}
}
