issn = {1748-3190},
journal = {Bioinspiration & biomimetics},
keywords = {Adaptation,Animals,Arthropods,Arthropods: physiology,Biological,Biomechanical Phenomena,Biomechanical Phenomena: methods,Biomimetics,Biomimetics: methods,Computer Simulation,Environment,Extremities,Extremities: physiology,Feedback,Feedback: physiology,Models,Physiological,Physiological: physiology,Robotics,Robotics: methods,Running,Running: physiology},
