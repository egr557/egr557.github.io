isbn = {0169409X (ISSN)},
issn = {0169409X},
journal = {Advanced Drug Delivery Reviews},
keywords = {Controlled release,Hydrogels,Lithography,Origami,Spatio-temporal,Three dimensional},
number = {14},
pages = {1579--1589},
pmid = {22425612},
publisher = {Elsevier B.V.},
title = {{Self-folding polymeric containers for encapsulation and delivery of drugs}},
url = {http://dx.doi.org/10.1016/j.addr.2012.02.012},
volume = {64},
year = {2012}
}
