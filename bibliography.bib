@article{Ohland2008,
abstract = {Ohland, Sheppard, Lichtenstein, Eris, Chachra and Layton (2008) report on large scale research into engineering student persistence. They firmly base their research on the study by Seymour and Hewitt. The authors are interested in the question how persistence, engagement and migration of engineering students compare to the expression of these factors by non engineering students. The study consists of two parts: the authors analysed data from the MIDFIELD data base that comprises record data for all undergraduate students at nine public universities in southeastern USA. They confronted the MIDFIELD data with data from the National Survey of Student Engagement (NSSE). The second part of the study was based on the Academic Pathway Study (APS) and looks for the relationship between student experiences and decisions to pursue an engineering degree. The question of persistence 1: Where do students go after matriculation? The figures for engineering are consistent with American national averages, as Seymour and Hewitt report. Ohland et al (2008) also provide comparison data for other disciplines, finding four-year persistence rates of 40% in STM, and 70% in arts, humanities and social sciences. The authors found that 53 percent of the students who matriculate in engineering persist in it. This is the largest percentage of persisters across all academic fields. The question of persistence 2 - Where do students come from who have been enrolled 8 semesters? Nearly 93% of students enrolled in engineering after 8 semesters had matriculated in engineering. This makes engineering very distinct from other groups of majors for which the same fraction ranges from 35-59%. The other technical fields, computer science and other STM, attract over 40% of their 8th semester student population from other majors. Whereas the number of students who first enrolled without declaring a choice of major and who were attracted to other STM and CS was less than 3% of the large undeclared population, these students comprise a noticeable percentage of those enrolled in other STM and CS in the 8th semester. Again, engineering does not attract a noticeable population of undeclared students. All other groups of majors attract engineering students in noticeable numbers. The question of engagement 1: National Survey of Student Engagement outcomes and engagement scales Many of the effect sizes on student engagement indicate that there is no relationship between college major and many of the student engagement and outcomes scales. The authors state that "It could be argued that [National Survey of Student Engagement] NSSE is a measure of engagement at the institutional level, not at the level of the individual major. However, we would expect the NSSE scales to reflect students' experiences in their courses and with their faculty, which likely vary by department. Certainly students in different majors have qualitatively distinct experiences based on class-sizes, structure of the major, and content. Yet, we found that students in different majors nevertheless rated the quality of their experiences similarly.” (Ohland, et al, 2008, page 270) The question of engagement 2: Time-on-task The results of time-on-task analyses indicate that engineering students spend time in ways similar to other students in other majors. Engineering students did report spending more time each week preparing for class. Engineering students did not differ appreciably from students in other majors in their on-campus employment, time spent commuting to class, or relaxing and socializing. Engineering students were similar to other STM and social science majors in terms of off-campus work or caring for dependents. In short, while statistical significance exists among most associations tested, the associations are very low and the explanatory value of those associations is minimal. Students do not differ in appreciable ways on these items based on their major. Engineering students are more engaged in community service than STM majors, also, 60 percent do some sort of internship or academic enrichment activity, compared to 45 percent in other majors. The relationship between engagement and persistence What remains to be explored is whether there is a relationship between engagement and persistence, including the temporal aspect of that relationship, and if that relationship is similarly consistent for students across the engineering and non-engineering majors. It is not possible to explore this relationship using the MIDFIELD or NSSE databases and the authors recommend that other researchers should attempt to research this in the future.},
author = {Ohland, Matthew W. and Sheppard, Sheri D. and Lichtenstein, Gary and Eris, Ozgur and Chachra, Debbie and Layton, Richard A.},
doi = {10.1002/j.2168-9830.2008.tb00978.x},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Ohland et al. - 2008 - Persistence, Engagement, and Migration in Engineering Programs.pdf:pdf},
isbn = {10694730},
issn = {10694730},
journal = {Journal of Engineering Education},
keywords = {engagement,migration,persistence},
month = {jul},
number = {3},
pages = {259--278},
title = {{Persistence, Engagement, and Migration in Engineering Programs}},
url = {http://doi.wiley.com/10.1002/j.2168-9830.2008.tb00978.x},
volume = {97},
year = {2008}
}
