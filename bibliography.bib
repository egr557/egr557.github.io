journal = {Bioinspiration & biomimetics},
keywords = {Animal Fins,Animal Fins: physiology,Animals,Biological,Biomimetic Materials,Biomimetics,Biomimetics: instrumentation,Biomimetics: methods,Computer Simulation,Computer-Aided Design,Equipment Design,Equipment Failure Analysis,Locomotion,Locomotion: physiology,Models,Robotics,Robotics: instrumentation,Robotics: methods,Turtles,Turtles: physiology},
number = {2},
pages = {026007},
pmid = {23612858},
title = {{Flipper-driven terrestrial locomotion of a sea turtle-inspired robot.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23612858},
volume = {8},
