issn = {00043702},
journal = {Artificial Intelligence},
month = {dec},
number = {3},
pages = {iii--iv},
title = {{Contents}},
url = {http://linkinghub.elsevier.com/retrieve/pii/0004370278900073},
volume = {11},
year = {1978}
