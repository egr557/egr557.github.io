isbn = {1748-3182},
issn = {1748-3182},
journal = {Bioinspiration & Biomimetics},
keywords = {Animal Fins,Animal Fins: physiology,Animals,Biological,Biomimetics,Biomimetics: instrumentation,Computer Simulation,Elastic Modulus,Elastic Modulus: physiology,Electric Fish,Electric Fish: physiology,Equipment Design,Equipment Failure Analysis,Mechanical,Models,Robotics,Robotics: instrumentation,Stress,Swimming,Swimming: physiology},
month = {jun},
