issn = {1476-4687},
journal = {Nature},
keywords = {DNA,DNA: chemistry,DNA: ultrastructure,Microscopy, Electron, Transmission,Nanostructures,Nanostructures: chemistry,Nanostructures: ultrastructure,Nanotechnology,Nanotechnology: methods,Nucleic Acid Conformation},
month = {may},
number = {7245},
pages = {414--8},
pmid = {19458720},
title = {{Self-assembly of DNA into nanoscale three-dimensional shapes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2688462&tool=pmcentrez&rendertype=abstract},
volume = {459},
year = {2009}
}
