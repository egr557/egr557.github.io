publisher = {IOP Publishing},
title = {{3D printed soft parallel actuator}},
url = {http://dx.doi.org/10.1088/1361-665X/aaab29 http://stacks.iop.org/0964-1726/27/i=4/a=045019?key=crossref.eb9961398bb0b7c3c3110566438b84bb},
