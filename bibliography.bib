file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Posa, Tobenkin, Tedrake - 2016 - Stability Analysis and Control of Rigid-Body Systems with Impacts and Friction.pdf:pdf},
issn = {00189286},
journal = {IEEE Transactions on Automatic Control},
keywords = {Control design,Lyapunov analysis and stability verification,rigid-body dynamics with impacts and friction,sumsof-squares (sos)},
number = {6},
