issn = {1748-3190},
journal = {Bioinspiration & Biomimetics},
keywords = {3D-printing,3d-printing,Manduca sexta,caterpillar,distributed control,locomotion,manduca sexta,model-free control,soft robots},
month = {mar},
number = {2},
