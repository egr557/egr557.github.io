issn = {0953816X},
journal = {European Journal of Neuroscience},
keywords = {Adult,Computer-Assisted,Fingers,Fingers: innervation,Frontal Lobe,Frontal Lobe: physiology,Hand,Hand: innervation,Humans,Image Processing,Magnetic Resonance Imaging,Male,Motor Cortex,Motor Cortex: physiology,Movement,Movement: physiology,Nerve Net,Nerve Net: physiology,Parietal Lobe,Parietal Lobe: physiology,Space Perception,Space Perception: physiology},
