file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Agheli et al. - 2014 - Design and fabrication of a foldable hexapod robot towards experimental swarm applications.pdf:pdf},
isbn = {978-1-4799-3685-4},
issn = {10504729},
keywords = {Fabrication,Legged locomotion,Optimization,Plastics,bars,built-in polyester fasteners,design flexibility,experimental swarm applications,fasteners,foldable hexapod design,foldable hexapod robot design,foldable hexapod robot fabrication,four-bar mechanisms,hexapod robot assembling,kinematic analysis,kinematics,laser beam cutting,laser cutter,origami-inspired foldable hexapod robot,robot flexibility,robot kinematics,swarm objectives},
month = {may},
pages = {2971--2976},
publisher = {IEEE},
title = {{Design and fabrication of a foldable hexapod robot towards experimental swarm applications}},
