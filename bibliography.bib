isbn = {1292-895X (Electronic)$\$r1292-8941 (Linking)},
issn = {12928941},
journal = {European Physical Journal E},
number = {2},
pmid = {22395533},
title = {{Motor-driven bacterial flagella and buckling instabilities}},
volume = {35},
year = {2012}
}
