pages = {678--685},
publisher = {Institute of Electrical and Electronics Engineers},
title = {{Modeling impact dynamics for robotic operations}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=1087851 http://ieeexplore.ieee.org/xpl/freeabs_all.jsp?arnumber=1087851},
volume = {4},
