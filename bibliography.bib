issn = {0027-8424},
number = {36},
pmid = {26294253},
title = {{Geometrically controlled snapping transitions in shells with curved creases}},
url = {http://arxiv.org/abs/1410.7038%0Ahttp://dx.doi.org/10.1073/pnas.1509228112},
volume = {112},
year = {2014}
}
