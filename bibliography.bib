isbn = {9781538680940},
issn = {21530866},
journal = {IEEE International Conference on Intelligent Robots and Systems},
pages = {5944--5951},
title = {{Sliding-Layer Laminates: A Robotic Material Enabling Robust and Adaptable Undulatory Locomotion}},
year = {2018}
}
