@article{Peraza-Hernandez2014,
abstract = {Origami, the ancient art of paper folding, has inspired the design of engineering devices and\r structures for decades. The underlying principles of origami are very general, which has led to\r applications ranging from cardboard containers to deployable space structures. More recently,\r researchers have become interested in the use of active materials (i.e., those that convert various\r forms of energy into mechanical work) to effect the desired folding behavior. When used in a\r suitable geometry, active materials allow engineers to create self-folding structures. Such\r structures are capable of performing folding and/or unfolding operations without being kinematically\r manipulated by external forces or moments. This is advantageous for many applications including\r space systems, underwater robotics, small scale devices, and self-assembling systems. This article\r is a survey and analysis of prior work on active self-folding structures as well as methods and\r tools available for the design of folding structures in general and self-folding structures in\r particular. The goal is to provide researchers and practitioners with a systematic view of the\r state-of-the-art in this important and evolving area. Unifying structural principles for active\r self-folding structures are identified and used as a basis for a quantitative and qualitative\r comparison of numerous classes of active materials. Design considerations specific to folded\r structures are examined, including the issues of crease pattern identification and fold kinematics.\r Although few tools have been created with active materials in mind, many of them are useful in the\r overall design process for active self-folding structures. Finally, the article concludes with a\r discussion of open questions for the field of origami-inspired engineering.},
author = {Peraza-Hernandez, Edwin A. and Hartl, Darren J. and Malak, Richard J. and Lagoudas, Dimitris C. and {Malak Jr}, Richard J and Lagoudas, Dimitris C. and Malak, Richard J. and Lagoudas, Dimitris C.},
