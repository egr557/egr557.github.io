@article{Dai2006,
abstract = {The Kutzbach-Grubler mobility criterion calculates the degrees of\nfreedom of a general mechanism. However the criterion can break down for\nmechanisms with special geometries, and in particular the class of\nso-called overconstrained parallel mechanisms. The problem is that the\ncriterion treats all constraints as active, even redundant constraints,\nwhich do not affect the mechanism degrees of freedom. In this paper we\nreveal a number of screw systems of a parallel mechanism, explore their\ninter-relationship and develop an original theoretical framework to\nrelate these screw systems to motion and constraints of a parallel\nmechanism to identify the platform constraints, mechanism constraints\nand redundant constraints. The screw system characteristics and\nrelationships are investigated for physical properties and a new\napproach to mobility analysis is proposed based on decompositions of\nmotion and constraint screw systems. New versions of the mobility\ncriterion are thus presented to eliminate the redundant constraints and\naccurately predict the platform degrees of freedom. Several examples of\noverconstrained mechanisms from the literature illustrate the results.},
author = {Dai, Jian S. and Huang, Zhen and Lipkin, Harvey},
doi = {10.1115/1.1901708},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Dai, Huang, Lipkin - 2006 - Mobility of Overconstrained Parallel Mechanisms.pdf:pdf},
issn = {10500472},
journal = {Journal of Mechanical Design},
keywords = {constraint redundancy,freedom reciprocal,mobility,motion,overconstraint,parallel mechanisms,platform,screw systems,screw theory,set-theoretic},
number = {1},
pages = {220},
title = {{Mobility of Overconstrained Parallel Mechanisms}},
volume = {128},
year = {2006}
}
@article{Daley2003,
abstract = {For a terrestrial animal to move in the complex natural environment, the limb muscles must modulate force and work performance to meet changing mechanical requirements; however, it is not clear whether this is accomplished via a collective shift in function by all limb muscles, or a division of labor among limb muscles. Do muscles differ in their ability to modulate force-length contractile function to meet the mechanical demands of different locomotor tasks? We explore this question by examining the in vivo force-length performance of the guinea fowl Numida meleagris lateral gastrocnemius (LG) and digital flexor-IV (DF-IV), during level and incline locomotion. During level locomotion, the LG and DF-IV exhibit differing muscle fascicle strain patterns: the LG shortens by 10-15% while developing force, whereas the DF-IV undergoes a stretch-shorten cycle with large strain amplitudes and small net strains of 1-8%. Furthermore, the DF-IV operates at higher muscle stresses (92-130 kPa, compared to 23-39 kPa for LG) and possesses a longer tendon, which allows the DF-IV tendon to recover greater elastic energy than the LG tendon. During incline locomotion, these muscles contribute only one-third of the energy expected for their mass, with the DF-IV exhibiting high stride-to-stride variability in work output. While the stretch-shorten cycle of the DF-IV muscle may allow more economic force production, it also leads to large changes in work output with small changes in the relative timing of force and strain. Thus, while the primary determinants of LG work are net strain and mean force, the primary determinant of DF-IV work is the phase relationship between force and strain. Our results suggest that, in addition to influencing a muscle's mechanical performance during steady level locomotion, morphology also affects its capacity and mechanism for altering work output for different locomotor tasks.},
author = {Daley, Monica A. and Biewener, Andrew A},
doi = {10.1242/jeb.00503},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Daley, Biewener - 2003 - Muscle force-length dynamics during level versus incline locomotion a comparison of in vivo performance of two.pdf:pdf},
isbn = {0022-0949},
issn = {0022-0949},
journal = {The Journal of experimental biology},
keywords = {Analysis of Variance,Animals,Biomechanical Phenomena,Birds,Birds: physiology,Electromyography,Locomotion,Locomotion: physiology,Lower Extremity,Lower Extremity: physiology,Muscle,Muscle Contraction,Muscle Contraction: physiology,Skeletal,Skeletal: physiology},
month = {sep},
number = {Pt 17},
pages = {2941--2958},
pmid = {12878663},
title = {{Muscle force-length dynamics during level versus incline locomotion: a comparison of in vivo performance of two guinea fowl ankle extensors.}},
url = {http://jeb.biologists.org/content/206/17/2941.abstract http://jeb.biologists.org/cgi/doi/10.1242/jeb.00503 http://www.ncbi.nlm.nih.gov/pubmed/12878663},
volume = {206},
