keywords = {Anatomic,Biological,Biomechanics,Computer Simulation,Hand,Hand Strength,Hand Strength: physiology,Hand: physiology,Humans,Models,Muscle,Skeletal,Skeletal: physiology},
month = {nov},
number = {1581},
