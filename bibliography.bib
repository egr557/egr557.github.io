journal = {Journal of the Robotics Society of Japan},
number = {4},
pages = {372--377},
title = {{PETMAN: A Humanoid Robot for Testing Chemical Protective Clothing}},
url = {http://japanlinkcenter.org/DN/JST.JSTAGE/jrsj/30.372?lang=en&from=CrossRef&type=abstract},
