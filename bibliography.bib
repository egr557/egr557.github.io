journal = {Journal of Terramechanics},
month = {apr},
number = {2},
pages = {133--152},
title = {{Digging and pushing lunar regolith: Classical soil mechanics and the forces needed for excavation and traction}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0022489806000577},
volume = {44},
year = {2007}
}
