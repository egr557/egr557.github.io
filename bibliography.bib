keywords = {Animal,Animals,Arm,Artificial Intelligence,Behavior,Biomechanics,Biophysical Phenomena,Biophysics,Brain,Brain Mapping,Brain: pathology,Electromyography,Electromyography: methods,Electrophysiology,Female,Hand,Hand Strength,Learning,Macaca,Models,Motor Activity,Motor Cortex,Motor Cortex: pathology,Movement,Neurological,Neurons,Neurons: metabolism,Primates,Psychomotor Performance,Psychomotor Performance: physiology,Robotics,Somatosensory Cortex,Somatosensory Cortex: pathology,Space Perception,Statistical,Theoretical,Time Factors},
