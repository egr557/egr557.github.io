journal = {Journal of neurophysiology},
keywords = {Animals,Female,Hand,Hand Strength,Hand Strength: physiology,Hand: physiology,Macaca mulatta,Male,Movement,Movement: physiology,Ocular,Ocular: physiology,Photic Stimulation,Photic Stimulation: methods,Posture,Posture: physiology,Psychomotor Performance,Psychomotor Performance: physiology,Vision},
month = {jun},
