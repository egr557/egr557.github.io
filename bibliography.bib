keywords = {Electromyography,Electromyography: methods,Hand,Hand Strength,Hand Strength: physiology,Hand: physiology,Humans,Muscle, Skeletal,Muscle, Skeletal: physiology,Posture,Posture: physiology},
month = {jul},
number = {1},
pages = {523--35},
pmid = {14973321},
title = {{Muscular and postural synergies of the human hand.}},
