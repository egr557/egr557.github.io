keywords = {Animals,Bionics,Bionics: methods,Brain,Brain Injuries,Brain Injuries: physiopathology,Brain Injuries: rehabilitation,Brain: physiology,Electrodes,Humans,Macaca mulatta,Macaca mulatta: physiology,Prostheses and Implants,Psychomotor Performance,Psychomotor Performance: physiology,User-Computer Interface},
month = {jul},
