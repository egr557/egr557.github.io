@article{Asadi-Eydivand2016,
abstract = {Fabrication of three-dimensional structures has gained increasing importance in the bone tissue engineering (BTE) field. Mechanical properties and permeability are two important requirement for BTE scaffolds. The mechanical properties of the scaffolds are highly dependent on the processing parameters. Layer thickness, delay time between spreading each powder layer, and printing orientation are the major factors that determine the porosity and compression strength of the 3D printed scaffold. In this study, the aggregated artificial neural network (AANN) was used to investigate the simultaneous effects of layer thickness, delay time between spreading each layer, and print orientation of porous structures on the compressive strength and porosity of scaffolds. Two optimization methods were applied to obtain the optimal 3D parameter settings for printing tiny porous structures as a real BTE problem. First, particle swarm optimization algorithm was implemented to obtain the optimum topology of the AANN. Then, Pareto front optimization was used to determine the optimal setting parameters for the fabrication of the scaffolds with required compressive strength and porosity. The results indicate the acceptable potential of the evolutionary strategies for the controlling and optimization of the 3DP process as a complicated engineering problem.},
author = {Asadi-Eydivand, Mitra and Solati-Hashjin, Mehran and Fathi, Alireza and Padashi, Mobin and {Abu Osman}, Noor Azuan},
doi = {10.1016/j.asoc.2015.11.011},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Asadi-Eydivand et al. - 2016 - Optimal design of a 3D-printed scaffold using intelligent evolutionary algorithms.pdf:pdf},
isbn = {1568-4946},
issn = {15684946},
journal = {Applied Soft Computing Journal},
keywords = {3D printer,Aggregated artificial neural network (AANN),Mechanical strength,Particle swarm optimization (PSO),Porous structure,Scaffolds},
pages = {36--47},
publisher = {Elsevier B.V.},
title = {{Optimal design of a 3D-printed scaffold using intelligent evolutionary algorithms}},
url = {http://dx.doi.org/10.1016/j.asoc.2015.11.011},
volume = {39},
year = {2016}
}
