isbn = {9781450318990},
keywords = {3D,interactive fabrication,laser cutting,physical prototyping,rapid prototyping},
pages = {2585},
publisher = {ACM Press},
title = {{LaserOrigami}},
url = {http://dl.acm.org/citation.cfm?doid=2470654.2481358},
year = {2013}
