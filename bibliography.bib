month = {nov},
number = {8},
pages = {1255--1269},
publisher = {Elsevier},
title = {{Control of whole finger manipulation utilizing frictionless sliding contact—theory and experiment}},
url = {http://www.sciencedirect.com/science/article/pii/S0094114X98000639 http://dx.doi.org/10.1016/S0094-114X(98)00063-9 http://linkinghub.elsevier.com/retrieve/pii/S0094114X98000639},
