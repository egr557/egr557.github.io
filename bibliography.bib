keywords = {Animals,Humans,Motor Cortex,Motor Cortex: physiology,Neural Pathways,Neural Pathways: physiology,Primates,Primates: physiology},
month = {apr},
number = {4},
pages = {283--296},
pmid = {9741757},
title = {{The organization of the cortical motor system: new concepts}},
