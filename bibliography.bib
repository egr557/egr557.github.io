keywords = {Artificial Limbs,Artificial Limbs: standards,Biomechanics,Computer-Aided Design,Evidence-Based Medicine,Hand,Hand Strength,Hand: physiology,Human Engineering,Humans,Models, Anatomic,Prosthesis Design,Research},
number = {5},
