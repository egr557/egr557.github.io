isbn = {0001-1452},
issn = {0001-1452},
journal = {AIAA Journal},
month = {apr},
number = {4},
pages = {806--823},
title = {{Low-Reynolds-Number Aerodynamics of a Flapping Rigid Flat Plate}},
url = {http://arc.aiaa.org/doi/10.2514/1.J050827},
volume = {49},
year = {2011}
