isbn = {9780470749081},
issn = {00457825},
journal = {Computer Methods in Applied Mechanics and Engineering},
keywords = {Boundary layers,CAD,Convergence,Finite element analysis,Fluid dynamics,Geometry,Internal layers,Mesh refinement,NURBS,Shells,Structural analysis,h-refinement,k-refinement,p-refinement},
month = {oct},
number = {39-41},
pages = {4135--4195},
pmid = {230451400006},
title = {{Isogeometric analysis: CAD, finite elements, NURBS, exact geometry and mesh refinement}},
