@inproceedings{Pratt2002a,
abstract = {The transformation from high level task specification to low level\nmotion control is a fundamental issue in sensorimotor control in\nanimals and robots. This paper describes a control scheme called\nvirtual model control that addresses this issue. Virtual model control\nis a motion control language that uses simulations of imagined mechanical\ncomponents to create forces, which are applied through real joint\ntorques, thereby creating the illusion that the virtual components\nare connected to the robot. Due to the intuitive nature of this technique,\ndesigning a virtual model controller requires the same skills as\ndesigning the mechanism itself. A high level control system can be\ncascaded with the low level virtual model controller to modulate\nthe parameters of the virtual mechanisms. Discrete commands from\nthe high level controller would then result in fluid motion. Virtual\nmodel control has been applied to a physical bipedal walking robot.\nA simple algorithm utilizing a simple set of virtual components has\nsuccessfully compelled the robot to walk continuously over level\nterrain},
author = {Pratt, J. and Dilworth, P. and Pratt, G.},
booktitle = {Proceedings of International Conference on Robotics and Automation},
doi = {10.1109/ROBOT.1997.620037},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Pratt, Dilworth, Pratt - 2002 - Virtual model control of a bipedal walking robot.pdf:pdf},
isbn = {0-7803-3612-7},
number = {April},
pages = {193--198},
publisher = {IEEE},
title = {{Virtual model control of a bipedal walking robot}},
url = {http://ieeexplore.ieee.org/document/620037/},
volume = {1},
year = {2002}
}
@inproceedings{Pratt1998a,
abstract = {Bipedal robots are difficult to analyze mathematically. However, successful control strategies can be discovered using simple physical intuition and can be described in simple terms. Five things have to happen for a planar bipedal robot to walk. Height has to be stabilized. Pitch has to be stabilized. Speed has to be stabilized. The swing leg has to move so that the feet are in locations which allow for the stability of height, pitch, and speed. Finally, transitions from support leg to support leg must occur at appropriate times. If these five objectives are achieved, the robot will walk. A number of different intuitive control strategies can be used to achieve each of these five objectives. Further, each strategy can be implemented in a variety of ways. We present several strategies for each objective which we have implemented on a bipedal walking robot. Using these simple intuitive strategies, we have compelled a seven link planar bipedal robot, called Spring Flamingo, to walk. The robot walks both slowly and quickly, walks over moderate obstacles, starts, and stops},
author = {Pratt, J. and Pratt, G.},
booktitle = {Proceedings. 1998 IEEE International Conference on Robotics and Automation (Cat. No.98CH36146)},
doi = {10.1109/ROBOT.1998.680611},
file = {:C\:/Users/danaukes/Documents/Mendeley Desktop/Pratt, Pratt - 1998 - Intuitive control of a planar bipedal walking robot.pdf:pdf},
isbn = {0-7803-4300-X},
keywords = {Control systems,Energy efficiency,Foot,Laboratories,Leg,Legged locomotion,Robot control,Spring Flamingo,Springs,Stability,height stabilization,intuitive control,pitch stabilization,planar bipedal walking robot,robust control,speed stabilization,velocity control},
month = {may},
pages = {2014--2021},
publisher = {IEEE},
title = {{Intuitive control of a planar bipedal walking robot}},
url = {http://ieeexplore.ieee.org/document/680611/},
volume = {3},
year = {1998}
}
