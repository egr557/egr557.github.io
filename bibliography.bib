title = {{Spatial Thinking and STEM Education}},
url = {http://www.sciencedirect.com/science/article/pii/B9780123942937000042 http://linkinghub.elsevier.com/retrieve/pii/B9780123942937000042},
volume = {57},
year = {2012}
